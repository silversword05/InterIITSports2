<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_marginTop="1dp"
    android:layout_marginStart="5dp"
    android:layout_marginEnd="5dp"
    android:paddingTop="10dp"
    android:paddingBottom="20dp"
    android:background="@drawable/team_individual_card_background"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">


    <TextView
        android:id="@+id/match_type_schedulei"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:fontFamily="@font/montserrat_regular"
        android:textAlignment="center"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        android:text="@string/points" />

    <TextView
        android:id="@+id/match_name_schedulei"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/match_type_schedulei"
        android:layout_alignParentStart="true"
        android:fontFamily="@font/montserratsemibold"
        android:textAlignment="center"
        android:textColor="@android:color/black"
        android:textSize="22sp"
        android:text="@string/points"/>

    <TextView
        android:id="@+id/match_venue_schedulei"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fontFamily="@font/montserrat_regular"
        android:textAlignment="center"
        android:textSize="18sp"
        android:text="@string/points"
        android:layout_below="@id/match_name_schedulei"
        android:textColor="@android:color/black"
        android:layout_alignParentEnd="true" />




</RelativeLayout>
